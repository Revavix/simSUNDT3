{
  "name": "simsundt3",
  "type": "module",
  "author": "Revavix AB & University West",
  "description": "SimSUNDT3 is a ultrasonic defect detection simulation tool",
  "private": true,
  "version": "0.1.0",
  "main": "src/index.cjs",
  "scripts": {
    "dev": "concurrently 'npm run web:watch' 'sleep 1 && npm run electron:start'",
    "web:watch": "vite",
    "electron:start": "NODE_ENV=development electron src/index.cjs",
    "build": "vite build && electron-builder --publish never",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.8.10",
    "@types/three": "^0.158.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^8.2.2",
    "electron": "^27.0.3",
    "electron-builder": "^24.6.4",
    "plotly.js-dist-min": "^2.27.0",
    "postcss": "^8.4.19",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "build": {
    "mac": {
      "publish": []
    },
    "linux": {
      "publish": []
    },
    "win": {
      "icon": "./src/assets/simsundt.ico",
      "publish": []
    },
    "files": [
      "src/**/*"
    ],
    "extraFiles": [
      "bin/**",
      "articles/**"
    ]
  },
  "dependencies": {
    "@threlte/core": "^6.1.0",
    "@threlte/extras": "^7.5.0",
    "@types/plotly.js-dist-min": "^2.3.3",
    "svelte-markdown": "^0.4.0",
    "three": "^0.158.0"
  }
}
