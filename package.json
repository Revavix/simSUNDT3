{
  "name": "simsundt3",
  "author": "Revavix AB & University West",
  "description": "SimSUNDT3 is a ultrasonic defect detection simulation tool",
  "private": true,
  "version": "0.1.0",
  "main": "src/index.js",
  "scripts": {
    "dev-win": "",
    "dev": "concurrently 'npm run web:watch' 'sleep 1 && npm run electron:start'",
    "web:watch": "vite",
    "electron:start": "NODE_ENV=development electron src",
    "build": "vite build && electron-builder --publish never",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^18.11.12",
    "@types/plotly.js": "^2.12.11",
    "@types/three": "^0.146.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "electron": "^21.2.2",
    "electron-builder": "^23.6.0",
    "plotly.js-dist-min": "^2.17.1",
    "postcss": "^8.4.19",
    "svelte": "^3.52.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^3.2.3"
  },
  "build": {
    "mac": {
      "publish": []
    },
    "linux": {
      "publish": []
    },
    "win": {
      "publish": []
    },
    "files": [
      "src/**/*"
    ]
  },
  "dependencies": {
    "@threlte/core": "^4.3.3",
    "@threlte/extras": "^4.5.1",
    "three": "^0.146.0"
  }
}
